{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "removeComments": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/assets": ["./src/assets"],
      "@/components": ["./src/components"],
      "@/config": ["./src/config"],
      "@/containers": ["./src/containers"],
      "@/navigators": ["./src/navigators"],
      "@/services": ["./src/services"],
      "@/store": ["./src/store"],
      "@/hooks": ["./src/hooks"],
      "@/translations": ["./src/translations"],
      "@/utils": ["./src/utils"],
      "@/models": ["./src/models"],
      "@/assets/*": ["./src/assets/*"],
      "@/components/*": ["./src/components/*"],
      "@/config/*": ["./src/config/*"],
      "@/containers/*": ["./src/containers/*"],
      "@/navigators/*": ["./src/navigators/*"],
      "@/services/*": ["./src/services/*"],
      "@/store/*": ["./src/store/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/translations/*": ["./src/translations/*"],
      "@/utils/*": ["./src/utils/*"],
      "@/models/*": ["./src/models/*"]
    }
  },
  "exclude": [
    "node_modules",
    "test",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "react-native.config.js",
    "wdyr.js"
  ]
}
